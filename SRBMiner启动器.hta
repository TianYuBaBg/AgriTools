            // è¿‡æ»¤åŒ¹é…çš„ç®—æ³•
            var matchedAlgorithms = [];
            for (var i = 0; i < commonAlgorithms.length; i++) {
                if (commonAlgorithms[i].toLowerCase().indexOf(input) > -1) {
                    matchedAlgorithms.push(commonAlgorithms[i]);
